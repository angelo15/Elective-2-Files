	<script>
			function writeinput1(){
				var input = document.getElementById("input1");
				var inputed = document.getElementsByTagName("input");
					for(var i = 0; i < inputed.length;i++){
						inputed[i].value = input.value;
					}
					
			}
			function writeinput2(){
